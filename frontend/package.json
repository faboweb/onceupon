{
  "name": "onceupon",
  "version": "0.0.1",
  "private": true,
  "description": "A blockchain based communal story telling game",
  "scripts": {
    "serve": "VUE_APP_NETWORK=mainnet && vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "deploy": "VUE_APP_NETWORK=mainnet && npm run build && firebase deploy --only hosting",
